{{>layouts/header}}

<form class="container" action="/members/create" method="post">
    <div class="mb-3">
        <label class="form-label">이메일</label>
        <input type="email" class="form-control" name="Member_Email">
    </div>
    <div class="mb-3">
        <label class="form-label">비밀번호</label>
        <input type="password" class="form-control" name="Member_Password">
    </div>
    <button class="btn btn-primary" type="submit">Submit</button>
    <button class="btn btn-primary" type="button" onclick="location.href='/hi'">Back</button>
</form>

{{>layouts/footer}}